SUBDESIGN mux_vmb
( 
	data[7..0]	:	input;
	result[3..0]	:	output;
	sel[0..0]	:	input;
) 
VARIABLE 
	result_node[3..0]	: WIRE;
	sel_node[0..0]	: WIRE;
	w_data52w[1..0]	: WIRE;
	w_data66w[1..0]	: WIRE;
	w_data78w[1..0]	: WIRE;
	w_data90w[1..0]	: WIRE;

BEGIN 
	result[] = result_node[];
	result_node[] = ( ((sel_node[] & w_data90w[1..1]) # ((! sel_node[]) & w_data90w[0..0])), ((sel_node[] & w_data78w[1..1]) # ((! sel_node[]) & w_data78w[0..0])), ((sel_node[] & w_data66w[1..1]) # ((! sel_node[]) & w_data66w[0..0])), ((sel_node[] & w_data52w[1..1]) # ((! sel_node[]) & w_data52w[0..0])));
	sel_node[] = ( sel[0..0]);
	w_data52w[] = ( data[4..4], data[0..0]);
	w_data66w[] = ( data[5..5], data[1..1]);
	w_data78w[] = ( data[6..6], data[2..2]);
	w_data90w[] = ( data[7..7], data[3..3]);
END;
